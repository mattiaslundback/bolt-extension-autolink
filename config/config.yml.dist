# Options for RSS autolink

# Set this to the filename of your CSS file. Set it to `false` to include no
# CSS, which means you should take care of the styling in your own .css files.

config:
  thumbnail_url: 'https://api.thumbalizr.com/?url=%url%&width=300'
  google_cse_url: 'https://www.googleapis.com/customsearch/v1?key=[key]&cx=%cse%&q=%search%&alt=atom'
